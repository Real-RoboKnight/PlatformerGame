<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?scenebuilder-classpath-element target/classes?>

<?import edu.asu.dylan.platformergame.sprites.entitys.Bear?>
<?import edu.asu.dylan.platformergame.sprites.entitys.Player?>
<?import edu.asu.dylan.platformergame.sprites.sprites.Fire?>
<?import edu.asu.dylan.platformergame.sprites.sprites.Wall?>
<?import edu.asu.dylan.platformergame.sprites.sprites.Win?>
<?import edu.asu.dylan.platformergame.sprites.entitys.RotatingObstacle?>
<?import javafx.scene.control.Spinner?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.asu.dylan.platformergame.Game">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <StackPane stylesheets="@styles.css">
        <ImageView fitHeight="1000.0" fitWidth="1900.0" pickOnBounds="true">
            <Image url="@Assets/GameBackground.jpg" />
        </ImageView>

        <Player fx:id="player" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="93.0" prefWidth="93.0" styleClass="player-style" StackPane.alignment="TOP_LEFT">
            <StackPane.margin>
                <Insets left="49.0" />
            </StackPane.margin>
        </Player>

        <!-- Bear on upper platform -->
        <Bear maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="217.0" prefWidth="120.0" styleClass="bear-style" StackPane.alignment="TOP_LEFT">
            <StackPane.margin>
                <Insets left="600.0" />
            </StackPane.margin>
        </Bear>

        <!-- Bear on lower platform -->
        <Bear maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="217.0" prefWidth="120.0" styleClass="bear-style" StackPane.alignment="BOTTOM_RIGHT">
            <StackPane.margin>
                <Insets bottom="200.0"  right="900"/>
            </StackPane.margin>
        </Bear>

        <!-- Fire on upper platform -->
        <Fire maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="45.0" prefWidth="93.0" rotate="180" styleClass="fire-style" StackPane.alignment="TOP_LEFT">
            <StackPane.margin>
                <Insets left="400.0" />
            </StackPane.margin>
        </Fire>
        <Fire maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="45.0" prefWidth="93.0" rotate="180" styleClass="fire-style" StackPane.alignment="TOP_LEFT">
        <StackPane.margin>
            <Insets left="1000.0" />
        </StackPane.margin>
    </Fire>

        <!-- Fire on lower platform -->
        <Fire maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="45.0" prefWidth="93.0" styleClass="fire-style" rotate="270" StackPane.alignment="BOTTOM_RIGHT">
            <StackPane.margin>
                <Insets right="220.0" bottom="30" />
            </StackPane.margin>
        </Fire>

        <Fire maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="45.0" prefWidth="93.0" styleClass="fire-style" rotate="180" StackPane.alignment="BOTTOM_RIGHT">
            <StackPane.margin>
                <Insets right="220.0" bottom="280" />
            </StackPane.margin>
        </Fire>


        <Fire maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="45.0" prefWidth="200.0" styleClass="fire-style" StackPane.alignment="BOTTOM_RIGHT">
            <StackPane.margin>
                <Insets bottom="120.0" right="52"/>
            </StackPane.margin>
        </Fire>
        <Win maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="47.0" prefWidth="47.0" styleClass="win-style" StackPane.alignment="BOTTOM_RIGHT">
            <StackPane.margin>
                <Insets bottom="280.0" right="20"/>
            </StackPane.margin>
        </Win>

        <RotatingObstacle maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="45.0" prefWidth="200.0" styleClass="fire-style" StackPane.alignment="CENTER_RIGHT">
            <StackPane.margin>
                <Insets right="-5.0" />
            </StackPane.margin>
        </RotatingObstacle>

        <Wall styleClass="wall-style" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
              minWidth="-Infinity" prefHeight="16.0" prefWidth="1900.0" StackPane.alignment="BOTTOM_CENTER"/>
        <Wall styleClass="wall-style" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="1000.0" prefWidth="16.0" StackPane.alignment="CENTER_RIGHT">
            <StackPane.margin>
                <Insets right="-5.0" />
            </StackPane.margin>
        </Wall>

        <Wall styleClass="wall-style" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="16.0" prefWidth="1900.0" StackPane.alignment="TOP_CENTER">
            <StackPane.margin>
                <Insets bottom="5.0" />
            </StackPane.margin>
        </Wall>
        <Wall styleClass="wall-style" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="1000.0" prefWidth="16.0" StackPane.alignment="CENTER_LEFT">
            <StackPane.margin>
                <Insets left="-5.0" />
            </StackPane.margin>
        </Wall>

        <Wall styleClass="wall-style" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="16.0" prefWidth="247.0" StackPane.alignment="BOTTOM_RIGHT">
            <StackPane.margin>
                <Insets bottom="119.0" />
            </StackPane.margin>
        </Wall>
        <Wall styleClass="wall-style" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="133.0" prefWidth="16.0" StackPane.alignment="BOTTOM_RIGHT">
            <StackPane.margin>
                <Insets right="247.0" />
            </StackPane.margin>
        </Wall>

        <!-- Upper left platform -->
        <Wall styleClass="wall-style" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="16.0" prefWidth="1500.0" StackPane.alignment="TOP_LEFT">
            <StackPane.margin>
                <Insets top="333.3" />
            </StackPane.margin>
        </Wall>

        <!-- Left wall for upper platform bear -->
        <Wall styleClass="wall-style" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="217.0" prefWidth="16.0" StackPane.alignment="TOP_LEFT">
            <StackPane.margin>
                <Insets top="116.3" />
                <Insets left="400.0" />
            </StackPane.margin>
        </Wall>

        <!-- Right wall for upper platform bear -->
        <Wall styleClass="wall-style" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="217.0" prefWidth="16.0" StackPane.alignment="TOP_LEFT">
            <StackPane.margin>
                <Insets top="116.3" />
                <Insets left="1100.0" />
            </StackPane.margin>
        </Wall>

        <!-- Middle right platform -->
        <Wall styleClass="wall-style" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="16.0" prefWidth="1500.0" StackPane.alignment="BOTTOM_RIGHT">
            <StackPane.margin>
                <Insets bottom="333.3" />
            </StackPane.margin>
        </Wall>

        <!-- Left wall for lower platform bear -->
        <Wall styleClass="wall-style" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="30.0" prefWidth="16.0" StackPane.alignment="BOTTOM_RIGHT">
            <StackPane.margin>
                <Insets bottom="300.0" right="1050.0" />
            </StackPane.margin>
        </Wall>
        <Wall styleClass="wall-style" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="217.0" prefWidth="16.0" StackPane.alignment="BOTTOM_RIGHT">
            <StackPane.margin>
                <Insets bottom="116.3" right="1500.0" />
            </StackPane.margin>
        </Wall>

        <!-- Right wall for lower platform bear -->
        <Wall styleClass="wall-style" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="217.0" prefWidth="16.0" StackPane.alignment="BOTTOM_RIGHT">
            <StackPane.margin>
                <Insets bottom="116.3" right="600.0" />
            </StackPane.margin>
        </Wall>

    </StackPane>
</VBox>
